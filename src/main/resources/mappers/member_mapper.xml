<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.givenness.backend.persistence.MemberMapper">

  <select id="selectMember" resultType="org.givenness.backend.domain.member.MemberAccount">
    SELECT
      member_id as memberId,
     `password`
      FROM member_account
    WHERE serial_num = #{serialNum}
  </select>

</mapper>

